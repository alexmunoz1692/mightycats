{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "globals": {
    "route": true,
    "axios": true,
    "google": true,
    "getViteEnv": true
  },
  "rules": {},
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ]
}
